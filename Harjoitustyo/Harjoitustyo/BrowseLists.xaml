<Page
    x:Class="Harjoitustyo.BrowseLists"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Harjoitustyo"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="Gray">
        <StackPanel>
            <TextBlock x:Name="MainHeader" FontSize="130" HorizontalAlignment="Left">Selaa valmiita inventaarioita</TextBlock>
            <Button x:Name="GoBack_button" Margin="20,50,0,0"  FontSize="22"  Height="72" Width="273" Click="GoBack_button_Click">Etusivulle</Button>
            
            <Button x:Name="ListFiles_button" Margin="20,50,0,0"  FontSize="22"  Height="72" Width="273" Click="ListFiles_button_Click">Näytä tiedostot</Button>
            <TextBlock x:Name="ListFiles">TÄHÄN NE TULEE</TextBlock>
        </StackPanel>
    </Grid>
</Page>
